@model IEnumerable<FacebookAutoPost.Models.Frequency>

@{
    ViewData["Title"] = "Index";
}

<head>
    <link rel="stylesheet" href="~/css/styles.css">
</head>

<h1>Frequency Table</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PageId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PostFrequency)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsRandom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cron)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PageId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PostFrequency)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsRandom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cron)
            </td>
        </tr>
}
    </tbody>
</table>
